<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="timeline_style">
        <attr name="marker" format="color|reference"/>
        <attr name="line" format="color|reference"/>
        <attr name="markerSize" format="dimension"/>
        <attr name="lineSize" format="dimension"/>
        <attr name="lineOrientation" format="enum">
            <enum name="horizontal" value="0"/>
            <enum name="vertical" value="1"/>
        </attr>
        <attr name="linePadding" format="dimension"/>
        <attr name="markerInCenter" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="XLeoEditText">
        <attr name="customStyle" format="enum">
            <enum name="rectangle" value="0"/>
            <enum name="line" value="1"/>
        </attr>
    </declare-styleable>
    <declare-styleable name="XLeoEditTextExtend">
        <attr name="preText" format="string"/>
        <attr name="textColor" format="color"/>
        <attr name="textSize" format="dimension"/>
        <attr name="hint" format="string"/>
        <attr name="hintColor" format="color"/>
        <attr name="text" format="string"/>
        <attr name="endButton" format="reference"/>
        <attr name="customStyleExtend" format="enum">
            <enum name="rectangle" value="0"/>
            <enum name="line" value="1"/>
        </attr>
        <attr name="inputType" format="enum">
            <enum name="number" value="0"/>
            <enum name="text" value="1"/>
        </attr>
        <attr name="maxLines" format="integer"/>
        <attr name="imeOptions" format="enum">
            <enum name="actionNext" value="0"/>
            <enum name="actionDone" value="1"/>
        </attr>
    </declare-styleable>

    <declare-styleable name="BothEndFitText">
        <attr name="isIndent" format="boolean"></attr>
    </declare-styleable>

</resources>